@charset "UTF-8";
@import url("https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@100;400;700&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Mukta:wght@500;700&display=swap");
/*====================================================================
Reset
====================================================================*/
html {
  color: #222;
  background: #fff;
}

html, body, div, figure, section, header, footer, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, table, tbody, tfoot, thead, tr, th, td {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-size: 100%;
  font-family: YuGothic, Helvetica Neue, Helvetica, Arial, "游ゴシック", "Hiragino Kaku Gothic ProN", Meiryo, sans-serif;
  line-height: 100%;
  vertical-align: baseline;
  background: transparent;
  box-sizing: border-box;
  -webkit-text-size-adjust: 100%;
}

fieldset,
img {
  border: 0;
}

/*
TODO think about hanlding inheritence differently, maybe letting IE6 fail a bit...
*/
address,
caption,
cite,
code,
dfn,
em,
th,
var {
  font-style: normal;
  font-weight: normal;
}

strong {
  font-style: normal;
}

ol,
ul {
  list-style: none;
}

caption,
th {
  text-align: left;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 100%;
  font-weight: normal;
}

q:before,
q:after {
  content: "";
}

abbr,
acronym {
  border: 0;
  font-variant: normal;
}

sup {
  vertical-align: text-top;
}

sub {
  vertical-align: text-bottom;
}

input,
textarea,
select {
  font-family: inherit;
  font-size: inherit;
  font-weight: inherit;
}

input,
textarea,
select {
  *font-size: 100%;
}

ol, ul {
  list-style: none;
}

a {
  text-decoration: none;
  cursor: pointer;
}

input,
button,
textarea,
select {
  margin: 0;
  padding: 0;
  background: none;
  border: none;
  border-radius: 0;
  outline: none;
  -webkit-appearance: none;
          appearance: none;
  line-height: 100%;
  box-sizing: border-box;
}

img {
  width: 100%;
  height: auto;
  vertical-align: bottom;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}

/*-----------------------------------------------
@keyframes
------------------------------------------------*/
@keyframes fadeIn {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes fadeInUp {
  0% {
    opacity: 0;
    transform: translateY(20px);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes pulse {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.1);
  }
  100% {
    transform: scale(1);
  }
}
@keyframes zoomOut {
  0% {
    transform: scale3d(1.2, 1.2, 1.2);
  }
  100% {
    transform: scale3d(1, 1, 1);
  }
}
@keyframes confetti {
  0% {
    opacity: 0;
    transform: rotateZ(15deg) rotateY(0deg) translate(0, 0);
  }
  25% {
    opacity: 1;
    transform: rotateZ(5deg) rotateY(360deg) translate(-5vw, 20vh);
  }
  50% {
    transform: rotateZ(15deg) rotateY(720deg) translate(5vw, 50vh);
  }
  75% {
    transform: rotateZ(5deg) rotateY(1080deg) translate(-10vw, 70vh);
  }
  86% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: rotateZ(15deg) rotateY(1440deg) translate(10vw, 80vh);
  }
}
/*====================================================================
siteHeader
====================================================================*/
.siteHeader {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-end;
  padding: 20px 0 0 20px;
  height: 93px;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  z-index: 5;
  border-bottom: 1px solid #dedede;
  min-width: 1280px;
}
.siteHeader::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: white;
  z-index: 8;
}
.logged-in .siteHeader {
  top: 32px;
}
.siteHeader__logo {
  position: relative;
  max-width: 483px;
  margin-bottom: 18px;
  z-index: 20;
  transition: all 0.3s ease-out 0s;
}
@media screen and (min-width: 769px) {
  .siteHeader__logo:hover {
    opacity: 0.6;
  }
}
.siteHeader__navMenu {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-end;
}
.siteHeader__navList {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-end;
  z-index: 10;
}
.siteHeader__navLi:not(:first-child) {
  margin-left: 30px;
}
.siteHeader .js-linkSerial {
  padding-right: 30px;
  margin-right: -30px;
  padding-left: 30px;
  margin-left: 0;
}
.siteHeader__navLink {
  position: relative;
  display: block;
  color: #292929;
  font-size: 16px;
  padding-bottom: 20px;
  font-weight: bold;
  letter-spacing: 0.1em;
  transition: all 0.3s ease-out 0s;
}
.siteHeader__navLink::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  display: block;
  width: 0;
  height: 2px;
  background: #2bac25;
  transition: all 0.3s ease-out 0s;
}
.siteHeader__navLink:hover::after, .siteHeader__navLink.u-hover::after {
  width: 100%;
}
.siteHeader__searchBtn {
  position: relative;
  padding-left: 50px;
  padding-right: 50px;
  padding-bottom: 20px;
  display: block;
  width: 125.56px;
  height: 55.56px;
  background-image: url("../images/common/ico_search.svg");
  background-size: 35.56px auto;
  background-repeat: no-repeat;
  background-position: top center;
  transition: all 0.2s ease-out 0s;
  z-index: 10;
}
@media screen and (min-width: 769px) {
  .siteHeader__searchBtn:hover, .siteHeader__searchBtn.u-hover {
    transform: scale(1.4);
    background-image: url("../images/common/ico_search_hover.svg");
  }
  .siteHeader__searchBtn:hover + .siteHeader__searchCont {
    top: calc(100% + 1px);
    opacity: 1;
  }
}
.siteHeader__tag {
  padding: 15px 0 15px;
  display: none;
  position: absolute;
  left: 0;
  top: 100%;
  width: 100%;
  background: #fff;
  z-index: 10;
}
.siteHeader__tag:hover {
  display: block;
}
.siteHeader__tagInner {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 8px 15px 0;
  margin: 0 auto;
  max-width: 1080px;
  color: #2bac25;
}
.siteHeader__tagName {
  margin-right: 20px;
  min-width: 82px;
  font-size: 17px;
  letter-spacing: 0.2em;
}
.siteHeader__tagCont {
  margin-top: -8px;
  margin-left: 20px;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
}
.siteHeader__tagLink {
  margin-bottom: 15px;
  padding: 10px;
  color: #2bac25;
  font-size: 13px;
  letter-spacing: 0.1em;
  border: 1px solid #2bac25;
  border-radius: 50px;
  box-sizing: border-box;
  transition: all 0.3s ease-out 0s;
}
.siteHeader__tagLink:hover {
  background: #2bac25;
  color: #fff;
}
.siteHeader__tagLink:not(:last-child) {
  margin-right: 15px;
}
.siteHeader__searchCont {
  padding: 28px;
  position: absolute;
  top: -100%;
  left: 0;
  width: 100%;
  background: #fff;
  opacity: 1;
  z-index: 6;
  transition: all 0.3s ease-out 0s;
}
.siteHeader__searchCont:hover {
  top: calc(100% + 1px);
  opacity: 1;
}
.siteHeader__searchInner {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  max-width: 1080px;
  color: #2bac25;
}
.siteHeader__searchTxt {
  padding: 11px 20px;
  width: 550px;
  height: 58px;
  font-size: 17px;
  line-height: 2;
  letter-spacing: 0.1em;
  border: 1px solid #2bac25;
  box-sizing: border-box;
}
.siteHeader__searchItem {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}
.siteHeader__searchSubmit {
  display: block;
  width: 58px;
  height: 58px;
  background-image: url("../images/common/ico_search_bar.svg");
  background-size: 100% auto;
  background-repeat: no-repeat;
  box-sizing: border-box;
  -webkit-appearance: none;
  outline: none;
  transition: all 0.3s ease-out 0s;
}
@media screen and (min-width: 769px) {
  .siteHeader__searchSubmit:hover {
    opacity: 0.6;
  }
}
.siteHeader__searchTags {
  margin-left: 25px;
  padding: 14px 16px;
  display: block;
  color: #2bac25;
  font-size: 16px;
  font-weight: bold;
  letter-spacing: 0.1em;
  border: 1px solid #2bac25;
  border-radius: 50px;
  transition: all 0.3s ease-out 0s;
}
.siteHeader__searchTags:hover {
  background: #2bac25;
  color: #fff;
}

